{
  "memory": 256,
  "ttl": 30,
  "timeout": 30,
  "minReplicas": 2,
  "maxReplicas": 4,
  "workers": 1,
  "events": {
    "onSettingsChanged": {
      "sender": "apps",
      "keys": ["settings"]
    }
  },
  "routes": {
    "keepAlive": {
      "path": "/_v/private/keep-alive",
      "public": true
    },
    "firebaseConfig": {
      "path": "/firebase-config.js",
      "public": true
    },
    "firebaseMessaging": {
      "path": "/firebase-messaging-sw.js",
      "public": true
    },
    "orderExport": {
      "path": "/themarketer/orders_export",
      "public": true
    },
    "brandsExport": {
      "path": "/brands_feed.xml",
      "public": true
    },
    "categoriesExport": {
      "path": "/categories_feed.xml",
      "public": true
    },
    "generateBrandsFeed": {
      "path": "/cron/generate/brands_feed/:cronToken",
      "public": true
    },
    "generateCategoriesFeed": {
      "path": "/cron/generate/categories_feed/:cronToken",
      "public": true
    },
    "generateProductsFeed": {
      "path": "/cron/generate/products_feed/:cronToken",
      "public": true
    },
    "removeSubscriber": {
      "path": "/themarketer/webhooks/unsubscribe",
      "public": true
    },
    "addSubscriber": {
      "path": "/themarketer/webhooks/subscribe",
      "public": true
    }
  }
}
